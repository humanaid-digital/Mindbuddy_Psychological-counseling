{
  "name": "mindbuddy-backend",
  "version": "1.0.0",
  "description": "마인드버디 심리상담 플랫폼 백엔드",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "dev": "NODE_ENV=development nodemon server.js",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "test:performance": "node scripts/performance-test.js",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "lint:report": "eslint . --ext .js --format json --output-file eslint-report.json",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "build": "echo 'No build step required for this Node.js application'",
    "db:init": "node scripts/init-database.js",
    "db:migrate": "node scripts/migrate.js",
    "logs:clear": "rm -rf logs/*.log",
    "docker:setup": "./scripts/docker-setup.sh",
    "docker:build": "docker build -f Dockerfile.backend -t mindbuddy-app .",
    "docker:dev": "docker-compose up -d app postgres redis frontend",
    "docker:full": "docker-compose --profile monitoring up -d",
    "docker:prod": "docker-compose --profile production --profile monitoring up -d",
    "docker:monolith": "docker-compose -f docker-compose.monolith.yml up -d",
    "docker:down": "docker-compose down",
    "docker:clean": "docker-compose down -v --remove-orphans",
    "docker:logs": "docker-compose logs -f",
    "docker:ps": "docker-compose ps",
    "docker:restart": "docker-compose restart",
    "docker:build-push": "./scripts/docker-build-push.sh",
    "docker:test-hub": "./scripts/docker-test.sh",
    "docker:ci-test": "./scripts/docker-ci-test.sh",
    "docker:demo": "./scripts/docker-demo.sh",
    "docker:hub-demo": "./scripts/docker-hub-demo.sh",
    "docker:hub-up": "docker-compose -f docker-compose.hub.yml up -d",
    "docker:hub-down": "docker-compose -f docker-compose.hub.yml down",
    "docker:hub-logs": "docker-compose -f docker-compose.hub.yml logs -f",
    "docker:hub-ps": "docker-compose -f docker-compose.hub.yml ps"
  },
  "keywords": [
    "counseling",
    "psychology",
    "mental-health"
  ],
  "author": "Human_AI_D",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.2.1",
    "helmet": "^7.0.0",
    "http-proxy-middleware": "^2.0.6",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "natural": "^6.5.0",
    "nodemailer": "^7.0.9",
    "pg": "^8.11.3",
    "redis": "^4.6.5",
    "sequelize": "^6.35.0",
    "socket.io": "^4.7.2",
    "uuid": "^9.0.0",
    "validator": "^13.15.15"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^9.1.6",
    "nodemon": "^3.0.1",
    "prettier": "^3.2.5",
    "supertest": "^7.0.0"
  }
}