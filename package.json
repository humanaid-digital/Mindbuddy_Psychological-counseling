{
  "name": "mindbuddy-backend",
  "version": "1.0.0",
  "description": "마인드버디 심리상담 플랫폼 백엔드",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "dev": "NODE_ENV=development nodemon server.js",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "lint:report": "eslint . --ext .js --format json --output-file eslint-report.json",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "db:init": "node scripts/init-database.js",
    "db:migrate": "node scripts/migrate.js",
    "logs:clear": "rm -rf logs/*.log"
  },
  "keywords": ["counseling", "psychology", "mental-health"],
  "author": "Human_AI_D",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "multer": "^1.4.5-lts.1",
    "express-validator": "^7.2.0",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.10.0",
    "socket.io": "^4.7.2",
    "nodemailer": "^6.9.15",
    "moment": "^2.29.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "@types/jest": "^29.5.12",
    "mongodb-memory-server": "^9.1.6",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5"
  }
}